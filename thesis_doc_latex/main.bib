@article{J.C.Corelli1960,
abstract = {Commercially available specimens of polycrystalline nand p-type lead telluride* and n- and p-type bismuth telluride* were irradiated in the Engineering Test Reactor at the National Reactor Test Station at Idaho Falls, Idahod The preirradiation thermoelectric properties are compared with the postirradiation properties. Duplicate samples irradiated with cadmium shields allowed a search to be made for the effects of transmutations. Total integrated flux-times of l05 X lo2' neutrons/cm2 (thermal) and 1.6 x lo1' neutrons/cm2 (fast, i.e., for neutrons having energies greater than 1 MeV) were accumulated by the specimens during irradiation. irradiation was 60 f 2DoC0 The specimen temperature during The effects of the irradiation on the thermoelectrical properties of these materials were found to be due almost entirely to fast neutrons insofar as most of the damage is healed during heat treatment. One of the expected nuclear reactions caused by thermal neutrons was verified experimentally by performing gamma-ray spectroscopy on each irradiated sample. Increases in the electrical resistivity for irradiated n-type PbTe and Bi2Te3 were found to meal at temperatures below 200OC. For irradiated n-type PbTe most of the increase in the electrical resistivity and Seebeck coefficient can be explained by the decrease in carrier concentration, which was determined by Hall coefficient measurements. Measurements of the electrical resistivity and Seebeck coefficient as a function of temperature for irradiated and unirradiated samples cut from the same ingot are given for all materials except p-type PbTe. For the latter sample only the measurements of the Seebeck coefficient are given.},
author = {{J. C. Corelli} and {R. T. Frost}},
mendeley-groups = {Thesis documents},
pages = {48},
title = {{WORK ON THIS ONE The Effects Of Reactor Irradiation On The Thermoelectric Properties Of Lead and Bismuth Tellurides}},
url = {https://www.osti.gov/scitech/servlets/purl/4136722},
year = {1960}
}


