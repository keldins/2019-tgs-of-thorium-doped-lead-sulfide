file = {:Volumes/Promovere/Mendeley{\_}Library3/Osherov et al/2007/Osherov et al.{\_}2007{\_}EPITAXY and orientation control in chemical solution deposited PbS and PbSe monocrystalline films.pdf:pdf},
isbn = {0022-0248},
issn = {00220248},
journal = {Journal of Crystal Growth},
keywords = {A1. Crystal morphology,A1. Growth models,A2. Growth from solutions,A3. Heteroepitaxy,A3. Superlattices,B2. Semiconducting lead compounds},
mendeley-groups = {PbS Thesis,Thesis documents},
pages = {169--178},
title = {{EPITAXY and orientation control in chemical solution deposited PbS and PbSe monocrystalline films}},
volume = {304},
year = {2007}
}
@article{Biton2014,
abstract = {We present a chemical bath deposition process for alloying PbS thin films with 232Th, a stable isotope of thorium, to provide a model system for radiation damage studies. Variation of deposition parameters such as temperature, reagent concentrations and time allows controlling the properties of the resulting films. Small amounts of incorporated thorium (0.5{\%}) strongly affected the surface topography and the orientation of the films and slowed down the growth rate. The Th appears to be incorporated as substitutional ions in the PbS lattice.},
