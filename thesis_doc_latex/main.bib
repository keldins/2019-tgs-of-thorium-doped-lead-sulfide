file = {:Volumes/Promovere/Mendeley{\_}Library3/Slonopas et al/2015/Slonopas et al.{\_}2015{\_}Chemically deposited nanocrystalline lead sulfide thin films with tunable properties for use in photovoltaics.pdf:pdf},
issn = {00134686},
journal = {Electrochimica Acta},
keywords = {Chemical bath deposition,Lead Sulfide Thesis,Nanocrystalline structure,Optical properties,PbS thin film,Semiconductor thin films},
mendeley-groups = {PbS Thesis,Thesis documents},
mendeley-tags = {Lead Sulfide Thesis},
pages = {140--149},
publisher = {Elsevier Ltd},
title = {{Chemically deposited nanocrystalline lead sulfide thin films with tunable properties for use in photovoltaics}},
url = {http://dx.doi.org/10.1016/j.electacta.2014.11.021},
volume = {151},
year = {2015}
}
