doi = {10.1016/S1369-7021(12)70220-1},
file = {:Volumes/Promovere/Mendeley{\_}Library3/Semonin, Luther, Beard/2012/Semonin, Luther, Beard{\_}2012{\_}Quantum dots for next-generation photovoltaics.pdf:pdf},
