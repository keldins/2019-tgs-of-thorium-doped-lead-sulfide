journal = {Biosensors and Bioelectronics},
keywords = {Au nanoparticles (Au-NPs),Differential pulse anodic stripping voltammetry (D,Nanoporous gold (NPG) electrode,PbS nanoparticles (PbS-NPs)},
mendeley-groups = {Thesis documents},
pages = {3113--3119},
title = {{Ultrasensitive electrochemical detection of DNA based on PbS nanoparticle tags and nanoporous gold electrode}},
